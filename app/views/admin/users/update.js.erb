$(".edit_user").html('').hide();
$("#user_<%= @user.id %>").html('<td><%= link_to @user do %><%= @user.name %><% end %></td><td><%= @user.email %></td><td><%= @user.role_id %></td><td><%= @user.status %></td><td><% if @user.role_id == 2 %><%= link_to edit_user_path(@user), remote: true do %><i class="fas fa-lock-open"></i><% end %><% else %><%= link_to edit_user_path(@user), remote: true do %><i class="fas fa-lock"></i><% end %><% end %></td><td><%= link_to edit_admin_user_path(@user), remote: true do %><i class="far fa-edit"></i><% end %></td><td><%= link_to @user, method: :delete, remote: true do %><i class="far fa-trash-alt"></i><% end %></td>');
