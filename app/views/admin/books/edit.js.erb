$("#book_<%= @book.id %>").find("td:eq(4)").html('<% if @book.status %>published<% else %>unpublished<% end %>');
$("#book_<%= @book.id %>").find("td:eq(6)").html('<%= time_ago_in_words(@book.updated_at) %> ago');
$("#book_<%= @book.id %>").find("td:eq(7)").html('<% if current_user.role_id == 3 %><% if @book.status %><%= link_to edit_admin_book_path(@book), remote: true do %><i class="fas fa-lock-open"></i><% end %><% else %><%= link_to edit_admin_book_path(@book), remote: true do %><i class="fas fa-lock"></i><% end %><% end %><% else %><% if current_user.role_id > @book.user.role_id || current_user.id == @book.user.id %><% if @book.status %><%= link_to edit_admin_book_path(@book), remote: true do %><i class="fas fa-lock-open"></i><% end %><% else %><%= link_to edit_admin_book_path(@book), remote: true do %><i class="fas fa-lock"></i><% end %><% end %><% end %><% end %>');
