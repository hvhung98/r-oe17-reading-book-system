$(".edit_user").html('').hide();
$("#user_<%= @user.id %>").html('<td><%= link_to @user do %><%= @user.name %><% end %></td><td><%= @user.email %></td><td><%= @user.role_id %></td><td><%= @user.status %></td><td><% if current_user.role_id == 3 %><% if @user.role_id == 2 %><%= link_to edit_user_path(@user), remote: true do %><i class="fas fa-lock-open"></i><% end %><% else %><%= link_to edit_user_path(@user), remote: true do %><i class="fas fa-lock"></i><% end %><% end %><% end %></td><td><% if current_user.role_id > @user.role_id || current_user.id == @user.id %><%= link_to edit_admin_user_path(@user), remote: true do %><i class="far fa-edit"></i><% end %><% end %></td><td><% if current_user.role_id > @user.role_id %><%= link_to admin_user_path(@user), method: :delete, remote: true do %><i class="far fa-trash-alt"></i><% end %><% end %></td>');
